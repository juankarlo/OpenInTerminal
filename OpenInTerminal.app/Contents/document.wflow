				<key>ActionParameters</key>
				<dict>
					<key>COMMAND_STRING</key>
					<string>if [ -z "$1" ] || [ "$1" = "/" ]
then
	osascript &lt;&lt;-EOF
	tell application "Terminal"
    	activate
    	do script ""
	end tell
	EOF
else
	for f in "$@"
	do
		if [ -d "$f" ]
		then
  			pathname=$f
		else
  			pathname=$(dirname "$f")
		fi
		echo "$pathname"
	done
fi</string>
